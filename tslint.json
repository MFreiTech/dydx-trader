{
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "rules": {
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false,
    "interface-name": true,
    "radix": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "prettier": true
  },
  "linterOptions": {
    "exclude": [
      "./node_modules/**"
    ]
  }
}